{
	"name": "cypress-testing-project",
	"version": "1.0.0",
	"description": "Automated software testing with Cypress",
	"main": "index.js",
	"scripts": {
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"cy:update-snapshots": "cypress run --env updateSnapshots=true",
		"cy:run-tests-with-no-failure": "cypress run --env failOnSnapshotDiff=false",
		"cy:percy": "percy exec --  cypress run"
	},
	"keywords": [
		"testing",
		"cypress",
		"automation",
		"visial testing",
		"regression"
	],
	"author": "Artur Alves",
	"license": "ISC",
	"dependencies": {
		"@percy/cli": "^1.12.0",
		"@percy/cypress": "^3.1.2",
		"cypress": "^10.10.0",
		"prettier": "^2.7.1"
	},
	"devDependencies": {
		"cypress-image-snapshot": "^4.0.1",
		"cypress-xpath": "^2.0.1"
	}
}
